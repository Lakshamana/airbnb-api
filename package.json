{
  "name": "adonis-api-app",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "Adonisjs boilerplate for API server with pre-configured JWT",
  "scripts": {
    "start": "nodemon server.js",
    "test": "node ace test",
    "startdb": "docker-compose -f docker/db.yml up -d",
